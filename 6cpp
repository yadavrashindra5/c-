#include<iostream>
using namespace std;
int main()
{
    for (int i=1;i<=5;++i)
    {
        for(int j=1;j<=5;++j)
        {
            if((i==1||i==5)&&(j==1||j==5))
                cout<<"0";
            else if((i==1||i==5)&&(j==2||j==3||j==4))
                cout<<"1";
            else if((i==2||i==3||i==4)&&(j==1||j==5))
                cout<<"1";
            else
                cout<<"0";

        }
        cout<<"\n";
    }
}
